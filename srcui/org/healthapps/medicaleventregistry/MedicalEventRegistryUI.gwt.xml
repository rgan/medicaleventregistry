<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.7.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/tags/1.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='medicaleventregistryui'>
    <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Inherit the default GWT style sheet.  You can change       -->
    <!-- the theme of your GWT application by uncommenting          -->
    <!-- any one of the following lines.                            -->
    <inherits name='com.google.gwt.user.theme.standard.Standard'/>
    <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
    <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

    <!-- Load the Google Maps GWT bindings from the gwt-google-apis project -->
    <!-- Added by projectCreator if you use the -addModule argument -->
    <inherits name="com.google.gwt.maps.GoogleMaps"/>
    <inherits name='com.google.gwt.http.HTTP'/>
    <inherits name="com.google.gwt.json.JSON"/>
    <!--
      If you want to deploy this application outside of localhost,
      you must obtain a Google Maps API key at:
      http://www.google.com/apis/maps/signup.html
      Replace the src attribute below with a URL that contains your key.
    -->
    <script src="http://maps.google.com/maps?gwt=1&amp;file=api&amp;v=2&amp;key=ABQIAAAACdRRXK7NXPnq1up2FKcnMBThqlnqxp5lhEyBZcw9hnxivHI88BRpzBQD5FyEFkt0hm7DLyj8jR0Wdg" />

    <entry-point class='org.healthapps.medicaleventregistry.client.MedicalEventRegistryUI'/>
</module>